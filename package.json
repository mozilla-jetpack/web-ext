{
  "name": "web-ext",
  "version": "6.8.0",
  "description": "A command line tool to help build, run, and test web extensions",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./util/adb": "./lib/util/adb.js",
    "./util/logger": "./lib/util/logger.js"
  },
  "files": [
    "index.js",
    "lib/**"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.9.0"
  },
  "engine-strict": true,
  "bin": {
    "web-ext": "bin/web-ext.js"
  },
  "scripts": {
    "build": "node scripts/build",
    "start": "node scripts/develop",
    "test": "node scripts/test",
    "test-coverage": "node scripts/test --coverage",
    "test-functional": "node scripts/test-functional",
    "audit-deps": "node ./scripts/audit-deps",
    "changelog": "npx conventional-changelog-cli -p angular -u",
    "changelog-lint": "commitlint --from master",
    "changelog-lint-from-stdin": "commitlint",
    "github-pr-title-lint": "node ./scripts/github-pr-title-lint",
    "gen-contributing-toc": "npx doctoc CONTRIBUTING.md"
  },
  "homepage": "https://github.com/mozilla/web-ext",
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/web-ext.git"
  },
  "bugs": {
    "url": "http://github.com/mozilla/web-ext/issues"
  },
  "keywords": [
    "web",
    "extensions",
    "web extensions",
    "browser extensions",
    "firefox",
    "mozilla",
    "add-ons",
    "google",
    "chrome",
    "opera"
  ],
  "dependencies": {
    "@babel/runtime": "7.18.3",
    "@devicefarmer/adbkit": "3.2.3",
    "addons-linter": "5.5.0",
    "bunyan": "1.8.15",
    "camelcase": "6.2.0",
    "chrome-launcher": "0.15.0",
    "debounce": "1.2.1",
    "decamelize": "5.0.1",
    "es6-error": "4.1.1",
    "firefox-profile": "4.2.2",
    "fs-extra": "9.1.0",
    "fx-runner": "1.2.0",
    "import-fresh": "3.3.0",
    "mkdirp": "1.0.4",
    "multimatch": "5.0.0",
    "mz": "2.7.0",
    "node-notifier": "9.0.1",
    "open": "7.4.2",
    "parse-json": "5.2.0",
    "promise-toolbox": "0.20.0",
    "sign-addon": "3.11.0",
    "source-map-support": "0.5.20",
    "strip-bom": "5.0.0",
    "strip-json-comments": "4.0.0",
    "tmp": "0.2.1",
    "update-notifier": "5.1.0",
    "watchpack": "2.1.1",
    "ws": "7.5.8",
    "yargs": "16.2.0",
    "zip-dir": "2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/eslint-parser": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-flow": "7.17.12",
    "@babel/register": "7.17.7",
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "babel-loader": "8.2.5",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "copy-dir": "1.3.0",
    "cross-env": "7.0.3",
    "deepcopy": "2.1.0",
    "eslint": "8.16.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-ft-flow": "2.0.1",
    "eslint-plugin-import": "2.26.0",
    "flow-bin": "0.165.1",
    "git-rev-sync": "3.0.2",
    "html-entities": "2.3.3",
    "mocha": "9.2.0",
    "nyc": "15.1.0",
    "prettyjson": "1.2.1",
    "shelljs": "0.8.5",
    "sinon": "12.0.1",
    "testdouble": "3.16.5",
    "yauzl": "2.10.0"
  },
  "author": "Kumar McMillan",
  "license": "MPL-2.0",
  "nyc": {
    "include": "src/**/*.js",
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
